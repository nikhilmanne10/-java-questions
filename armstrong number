import java.util.* ;
import java.io.*; 
import java.util.Scanner;
import java.math.*;
public class stringg {

        public static void main(String[] args){
        	int rem,result=0,temp;
        	int num=152;
        	temp=num;
        	while(num>0) {
        		rem=num%10;
        		num=num/10;
        		result=result+(rem*rem*rem);
        	}
        	if(temp==result) {
        		System.out.println("armstrong number");
        	}
        	else {
        		System.out.println("not a ");
        	}
        	
        	}


        }
    
